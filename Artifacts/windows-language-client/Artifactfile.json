{
  "$schema": "https://raw.githubusercontent.com/Azure/azure-devtestlab/master/schemas/2016-11-28/dtlArtifacts.json",
  "title": "Language Windows Client fr-CA",
  "description": "Sets the display language to French Canadian.  Only works with Windows 11 22H2.",
  "publisher": "Eric Côté",
  "tags": [
    "Français",
    "Francais",
    "French",
    "Canada",
    "Regional Settings",
    "Language"
  ],
  "iconUri": "https://github.com/EricCote/DevTestLabs/raw/master/Images/windows11.svg",
  "targetOsType": "Windows",
  "parameters": {
    "lang": {
      "type": "bool",
      "displayName": "OnlineDownload",
      "description": "Should we use local files or download online files",
      "defaultValue": false
    }
  },
  "runCommand": {
    "commandToExecute": "[concat('powershell.exe -ExecutionPolicy bypass -File Set-Langwin11.ps1 -OnlineDownload:$', parameters('OnlineDownload') )]"
  }
}
